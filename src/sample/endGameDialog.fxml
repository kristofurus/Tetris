<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.Label?>
<FlowPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.EndGameDialog"
          orientation="VERTICAL" alignment="CENTER" vgap="10" prefHeight="275.0" prefWidth="300.0">
    <Label text="Game Over" font="size=20"/>
    <Label fx:id="scoreLabel"/>
    <Label fx:id="timeLabel"/>
    <Button fx:id="playAgainButton" text="Return" onAction="#handlePlayAgainButton"/>
    <Button fx:id="exitButton" text="Exit" onAction="#handleExitButton"/>
</FlowPane>
